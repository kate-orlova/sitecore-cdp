@using Foundation.SitecoreCDP.Configuration

<script>
    // Define the Boxever queue
    var _boxeverq = _boxeverq || [];

    // Define the Boxever settings
    var _boxever_settings = {
        client_key: "@ConfigSettings.ClientKey",
        target: "@ConfigSettings.StreamAPITarget",
        cookie_domain: "@ConfigSettings.CookieDomain",
        pointOfSale: "@ConfigSettings.POS",
        web_flow_target: "@ConfigSettings.WebFlowTarget",
        web_flow_config: { async: false, defer: false },
        javascriptLibraryVersion: "1.4.9"
    };

    // Load the Boxever JavaScript Library asynchronously
    (function () {
        var s = document.createElement("script");
        s.type = "text/javascript";
        s.async = true;
        s.src = "https://d1mj578wat5n4o.cloudfront.net/boxever-" + window._boxever_settings.javascriptLibraryVersion + ".min.js";
        var x = document.getElementsByTagName("script")[0]; x.parentNode.insertBefore(s, x);
    })();
</script>